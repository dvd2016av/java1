package java1;
import java.awt.*;
import javax.swing.*;
import javax.swing.Button.*;

public class java1{
        public static void main(String[]args){
        JFrame yy =new JFrame("窗口1");
        JButton yht =new JButton("按钮一");
        yy.setText("窗口1");
        yht.setText("按钮一");
        yy.add(yht);
}
        
